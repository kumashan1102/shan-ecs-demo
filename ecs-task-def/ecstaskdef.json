{
    "family": "shan-ecs-demo-fargate",
    "taskRoleArn": "arn:aws:iam::121196576469:role/shan-ecs-task-iam-role",
    "executionRoleArn": "arn:aws:iam::121196576469:role/shan-ecs-task-exec-role",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "shan-ecs-demo-fargate",
            "image": "121196576469.dkr.ecr.region.amazonaws.com/shan-ecs-ecr-repo:latest",
            "cpu": 1024,
            "memory": 2048,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "name": "nodepocport",
                    "appProtocol": "HTTP"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fargate-node-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    }
}